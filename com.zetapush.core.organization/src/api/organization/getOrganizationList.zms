macroscript getOrganizationList(
) {
	/** Get group list from API */
	const groups = sudo zpRecipeUtils::GLOBAL_OWNER zpServiceGroupsOrganization.allGroups({});
	/** Delegate get all user groups */
	const { list } = sudo zpRecipeUtils::GLOBAL_OWNER await zpRecipeGroup::core_group__getGroupListByService({
		groups,
		zpService: zpServiceGroupsOrganization
	});
} return {
	Organization[] list
} on channel __selfName
