/**
 *
 */
macroscript addRoomMember(
	/** Room id */ @ValidRoomId string id,
	/** User key */ @NotNull string member
) {
	const response = await zpRecipeGroup::core_group__addGroupMemberByService({
		id,
		member,
		zpService: zpServiceGroups
	});
} broadcast(response.resource) response on channel __selfName
