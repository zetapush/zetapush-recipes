@HasPermission(permission = #getPermissionList)
macroscript getPermissionList(
) {
	/** Get group list from API */
	const groups = sudo zpRecipeUtils::GLOBAL_OWNER zpServiceGroupsPermission.allGroups({});
	/** Delegate get all user groups */
	const { list } = sudo zpRecipeUtils::GLOBAL_OWNER await zpRecipeGroup::core_group__getGroupListByService({
		groups,
		zpService: zpServiceGroupsPermission
	});
} return {
	Permission[] list
} on channel __selfName
