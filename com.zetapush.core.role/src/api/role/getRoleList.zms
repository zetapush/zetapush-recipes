@HasPermission(permission = #getRoleList)
macroscript getRoleList(
) {
	/** Get group list from API */
	const groups = sudo zpRecipeUtils::GLOBAL_OWNER zpServiceGroupsRole.allGroups({});
	/** Delegate get all user groups */
	const { list } = sudo zpRecipeUtils::GLOBAL_OWNER await zpRecipeGroup::core_group__getGroupListByService({
		groups,
		zpService: zpServiceGroupsRole
	});
} return {
	Role[] list
} on channel __selfName
