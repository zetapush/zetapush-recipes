/**
 * Get metadata
 */
macroscript getMetadata(
	/** Metadata id */ @NotNull string id
) {
	/** Get Stored Object */
	const { revision, update, value } = await core_utils__getStoredObject({
		id, table: TABLE_METADATA, zpService: zpServiceGda
	});
} return new Metadata(id, revision, update, value) on channel __selfName
