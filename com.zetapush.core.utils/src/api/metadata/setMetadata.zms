/**
 * Set metadata
 */
macroscript setMetadata(
	/** Metadata id */ @NotNull string id,
	/** Metadata value */ @NotNull map metadata
) {
	/** Store metadata */
	const { revision, update, value } = await core_utils__setStoredObject({
		id, value: metadata, table: TABLE_METADATA, zpService: zpServiceGda
	});
} return new Metadata(id, revision, update, value) on channel __selfName
