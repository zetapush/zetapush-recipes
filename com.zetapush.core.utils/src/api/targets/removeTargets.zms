/**
 * Remove targets associated to a specific id
 * Targets represent a list of string (group id or user key)
 */
macroscript removeTargets(
	/** Targets id */ @NotNull string id
) {
	/** Remove stored object */
	await core_utils__removeStoredObject({
		id, table: TABLE_TARGETS, zpService: zpServiceGda
	});
} broadcast { string id } on channel __selfName
