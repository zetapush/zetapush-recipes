/**
 * Get targets associated to a specific id
 * Targets represent a list of string (group id or user key) 
 */
macroscript getTargets(
	/** Targets id */ @NotNull string id
) {
	/** Get Stored Object */
	const { revision, update, value } = await core_utils__getStoredObject({
		id, table: TABLE_TARGETS, zpService: zpServiceGda
	});
	/** Destructure result properties */
	value = usr:reduce_map_to_array(value);
} return new Targets(id, revision, update, value) on channel __selfName
