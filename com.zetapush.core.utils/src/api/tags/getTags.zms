macroscript getTags(
	/** Tags id */ @NotNull string id
) {
	/** Get Stored Object */
	const { revision, update, value } = await core_utils__getStoredObject({
		id, table: TABLE_TAGS, zpService: zpServiceGda
	});
	/** Destructure result properties */
	value = usr:reduce_map_to_array(value);
} return new Tags(id, revision, update, value) on channel __selfName
