/**
 * core_file__getFileEntryByService
 */
macroscript requestFileUpload(
	@NotNull string contentType,
	@NotNull string folder,
	string owner
) {
	/** Manage default parameters */
	owner = owner ?: __userKey;
	trace(#requestFileUpload, contentType, folder, owner);
	/** Request file upload */
	const response = await core_file__requestFileUploadByService({ contentType, folder, owner, zpService: zpServiceFile });
} broadcast response on channel __selfName
