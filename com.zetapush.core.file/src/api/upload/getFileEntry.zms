/**
 *
 */
macroscript getFileEntry(
	/** */ @NotNull string path,
	/** */ string owner
) {
	/** Manage default parameters */
	owner = owner ?: __userKey;
	trace(#getFileEntry, path);
	/** Get file upload */
	const response = await core_file__getFileEntryByService({ path, owner, zpService: zpServiceFile });
} return response on channel __selfName
