/**
 *	Macroscript used to launch the timeout that simulate a device ringing
 */ 
macroscript configureCallTimeout(
	/** ID of the call */ @NotNull string id,
	/** Group ID */ @NotNull string groupId
) {
	
	timer.setTimeout({
		"parameter" : {
			"name": "timeoutCall",
			"parameters": { "id": id, "group": [groupId, __userKey] }	
		}, 
		"deploymentId": NAME_DEPLOY_ID_MACRO, 
		"verb" : "func", 
		"delay" : TIMEOUT_CALL_SECONDS,
		"loud": true
	});
	
} return { id } on channel __selfName